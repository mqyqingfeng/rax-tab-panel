{
  "name": "rax-tab-panel",
  "version": "1.0.1-beta",
  "description": "rax",
  "author": "",
  "main": "src/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./lib && rm -rf ./package-lock.json",
    "build": "npm run clean && rax-scripts build --type 'component'",
    "start": "rax-scripts start --type 'component' -p 8000",
    "prepublishOnly": "npm run build",
    "miniapp": "rax-scripts start --type 'component-miniapp'",
    "test": "rax-scripts test",
    "lint": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx ./src"
  },
  "keywords": [
    "Rax",
    "rax-component"
  ],
  "engines": {
    "npm": ">=3.0.0"
  },
  "dependencies": {
    "driver-universal": "^1.0.0",
    "object-assign": "^4.1.1",
    "rax-find-dom-node": "^1.0.0",
    "rax-link": "^1.0.1-beta.0",
    "rax-listview": "^0.6.5",
    "rax-proptypes": "^1.0.0",
    "rax-recyclerview": "^1.0.0-beta.1",
    "rax-scrollview": "^1.0.0-beta.6",
    "rax-text": "^1.0.1-beta.0",
    "rax-view": "^1.0.1-beta.8",
    "universal-env": "^0.6.6",
    "universal-transition": "^1.0.0-beta.1",
    "weex-bindingx": "^0.0.49"
  },
  "peerDependencies": {
    "rax": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.12",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "csstype": "^2.6.4",
    "driver-dom": "^1.0.0",
    "driver-weex": "^1.0.0",
    "eslint": "^5.16.0",
    "eslint-config-rax": "^0.0.0",
    "rax": "^1.0.0",
    "rax-scripts": "1.2.1",
    "rax-test-renderer": "^1.0.0"
  },
  "miniappConfig": {
    "main": "lib/miniapp/index",
    "subComponents": {
      "TabController": {
        "tagNameMap": "rax-tabpanel"
      },
      "TabPanel": {
        "tagNameMap": "rax-tabpanel-item"
      }
    }
  }
}
